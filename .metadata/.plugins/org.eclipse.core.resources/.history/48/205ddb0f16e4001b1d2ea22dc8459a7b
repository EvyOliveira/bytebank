package bytebank;

public class TestaMetodo {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Conta contaDaEvy = new Conta();
		contaDaEvy.saldo = 100;
		contaDaEvy.deposita(50);
		System.out.println(contaDaEvy.saldo);
		
		contaDaEvy.saca(20);
		System.out.println(contaDaEvy.saldo]);
	}
}
